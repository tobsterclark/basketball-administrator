{
    "name": "functions",
    "scripts": {
        "lint": "eslint --ext .js,.ts .",
        "build": "tsc && shx mkdir -p lib/resources && shx cp -R src/resources/* lib/resources/",
        "build-watch": "shx mkdir -p lib/resources && shx cp -R src/resources/* lib/resources/ && tsc --watch",
        "serve": "npm run build-watch | firebase emulators:start",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "18"
    },
    "main": "lib/index.js",
    "dependencies": {
        "@prisma/client": "^5.14.0",
        "@tobsterclark/prisma-orm": ">=1.1.1",
        "better-firebase-functions": "^6.0.0",
        "build": "^0.1.4",
        "firebase-admin": "^11.11.1",
        "firebase-functions": "^4.6.0",
        "pdf-lib": "^1.17.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "eslint": "^8.9.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.25.4",
        "firebase-functions-test": "^3.1.0",
        "shx": "^0.3.4",
        "typescript": "^4.9.0"
    },
    "private": true
}
